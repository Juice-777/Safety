@model IEnumerable <Safety.Models.Speciality>

<div id="searchresults">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeTest.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TypeTest.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.ActionLink("Редакт", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Инфо", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Удалить", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</div>